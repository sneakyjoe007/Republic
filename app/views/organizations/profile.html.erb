<p id="notice"><%= notice %></p>

<h2> Profile Page </h2>
<div class="center"> 
	<h3><%= current_organization.name %></h3>
</div>

<div class="row">
	<div class="col-md-4">
		<div class="thumbnail">
			<%= image_tag "default.jpeg" %>
		</div>
	</div>
	<div class="col-md-8">
		  <p> This is a placeholder for Profile Stuff </p>
	</div>
</div>

<table class="table table-striped table-bordered">
	<tr>
		<th class="center">Event Name</th>
		<th class="center">Users Registered</th>
	</tr>

	<% @events.each do |event| %>
	 <tr>
	 	<td class="center"><%= link_to event.name, event %> </td>
	 	<td class="center"><%= event.volunteers_current %></td>
	 </tr>
	<% end %>
</table>
